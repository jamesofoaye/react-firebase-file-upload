{
  "name": "react-firebase-file-upload",
  "version": "3.0.0",
  "description": " Firebase File Uploader",
  "author": "jamesofoaye",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesofoaye/react-firebase-file-upload.git"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.1",
    "@types/react": "^18.2.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-standard": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.3",
    "jest": "^28.1.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^28.1.1",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.4"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "firebase": "^9.13.0"
  },
  "bugs": {
    "url": "https://github.com/jamesofoaye/react-firebase-file-upload/issues"
  },
  "homepage": "https://github.com/jamesofoaye/react-firebase-file-upload#readme",
  "directories": {
    "example": "example"
  },
  "keywords": [
    "firebase",
    "image",
    "uploader",
    "firebase",
    "file",
    "uploader",
    "firebase",
    "storage",
    "uploader"
